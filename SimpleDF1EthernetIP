ldriver 3.0
:T=Param1[1,1]
:N=Param1[2,':']
:E=Param1[':','/']
:s1=Param1['/','']
:S=0
:barra=Param1['/','/']
:ponto1=Param1['.','.']
:letratimer1=Param1['.','']
:ETimer=Param1[':','.']
:qtde=TotalDeItens

?T='O'
:qtde=1

?T='O'
?ponto1=1
:N=0
:B=Param1[':','.']
:E=Param1['.','/']
:ponto1=0
:letratimer1=0
:ETimer=0

?T='I'
:qtde=1

?T='I'
?ponto1=1
:N=1
:B=Param1[':','.']
