:letratimer1=0
:ETimer=0

:TimeOut1=5
:status=0
:context=0
:options=0
:CIP=0

?qtde>30
:qtde=30

?qtde=0
:qtde=1

:Try=1
:NoCloseComTry=0
:NoCloseCom=0
:TimeOut=1000
:TimeOutByte=50
:CacheTimeOut=100
:p1=44+IPCONFLEN
:p2=43+IPCONFLEN
:BufferDontCare=p1
:BufferDontCareBytes=2

:Especial=int(E/qtde)

$GETGLOBAL(TNS)

?TNS=0
:TNS=17

:TNS=TNS+1

?TNS>271
:TNS=17

$SETGLOBAL(TNS)

:tipotamanho=2
:tamanho=tipotamanho*qtde
:DST=Param2

:headerlen=27+IPCONFLEN

:session=0
:x1=0

$GETGLOBAL(session)

/////////:session=0
:erro=0

?session=0
:context=0
:CacheTimeOut=0
